USER #!/bin/sh
USER #
USER # An example hook script to block unannotated tags from entering.
USER # Called by "git receive-pack" with arguments: refname sha1-old sha1-new
USER #
USER # To enable this hook, rename this file to "update".
USER #
USER # Config
USER # ------
USER # hooks.allowunannotated
USER #   This boolean sets whether unannotated tags will be allowed into the
USER #   repository.  By default they won't be.
USER # hooks.allowdeletetag
USER #   This boolean sets whether deleting tags will be allowed in the
USER #   repository.  By default they won't be.
USER # hooks.allowmodifytag
USER #   This boolean sets whether a tag may be modified after creation. By default
USER #   it won't be.
USER # hooks.allowdeletebranch
USER #   This boolean sets whether deleting branches will be allowed in the
USER #   repository.  By default they won't be.
USER # hooks.denycreatebranch
USER #   This boolean sets whether remotely creating branches will be denied
USER #   in the repository.  By default this is allowed.
USER #
USER 
USER # --- Command line
USER refname="$1"
USER oldrev="$2"
USER newrev="$3"
USER 
USER # --- Safety check
USER if [ -z "$GIT_DIR" ]; then
USER 	echo "Don't run this script from the command line." >&2
USER 	echo " (if you want, you could supply GIT_DIR then run" >&2
USER 	echo "  $0 <ref> <oldrev> <newrev>)" >&2
USER 	exit 1
USER fi
USER 
USER if [ -z "$refname" -o -z "$oldrev" -o -z "$newrev" ]; then
USER 	echo "usage: $0 <ref> <oldrev> <newrev>" >&2
USER 	exit 1
USER fi
USER 
USER # --- Config
USER allowunannotated=$(git config --type=bool hooks.allowunannotated)
USER allowdeletebranch=$(git config --type=bool hooks.allowdeletebranch)
USER denycreatebranch=$(git config --type=bool hooks.denycreatebranch)
USER allowdeletetag=$(git config --type=bool hooks.allowdeletetag)
USER allowmodifytag=$(git config --type=bool hooks.allowmodifytag)
USER 
USER # check for no description
USER projectdesc=$(sed -e '1q' "$GIT_DIR/description")
USER case "$projectdesc" in
USER "Unnamed repository"* | "")
USER 	echo "*** Project description file hasn't been set" >&2
USER 	exit 1
USER 	;;
USER esac
USER 
USER # --- Check types
USER # if $newrev is 0000...0000, it's a commit to delete a ref.
USER zero=$(git hash-object --stdin </dev/null | tr '[0-9a-f]' '0')
USER if [ "$newrev" = "$zero" ]; then
USER 	newrev_type=delete
USER else
USER 	newrev_type=$(git cat-file -t $newrev)
USER fi
USER 
USER case "$refname","$newrev_type" in
USER 	refs/tags/*,commit)
USER 		# un-annotated tag
USER 		short_refname=${refname##refs/tags/}
USER 		if [ "$allowunannotated" != "true" ]; then
USER 			echo "*** The un-annotated tag, $short_refname, is not allowed in this repository" >&2
USER 			echo "*** Use 'git tag [ -a | -s ]' for tags you want to propagate." >&2
USER 			exit 1
USER 		fi
USER 		;;
USER 	refs/tags/*,delete)
USER 		# delete tag
USER 		if [ "$allowdeletetag" != "true" ]; then
USER 			echo "*** Deleting a tag is not allowed in this repository" >&2
USER 			exit 1
USER 		fi
USER 		;;
USER 	refs/tags/*,tag)
USER 		# annotated tag
USER 		if [ "$allowmodifytag" != "true" ] && git rev-parse $refname > /dev/null 2>&1
USER 		then
USER 			echo "*** Tag '$refname' already exists." >&2
USER 			echo "*** Modifying a tag is not allowed in this repository." >&2
USER 			exit 1
USER 		fi
USER 		;;
USER 	refs/heads/*,commit)
USER 		# branch
USER 		if [ "$oldrev" = "$zero" -a "$denycreatebranch" = "true" ]; then
USER 			echo "*** Creating a branch is not allowed in this repository" >&2
USER 			exit 1
USER 		fi
USER 		;;
USER 	refs/heads/*,delete)
USER 		# delete branch
USER 		if [ "$allowdeletebranch" != "true" ]; then
USER 			echo "*** Deleting a branch is not allowed in this repository" >&2
USER 			exit 1
USER 		fi
USER 		;;
USER 	refs/remotes/*,commit)
USER 		# tracking branch
USER 		;;
USER 	refs/remotes/*,delete)
USER 		# delete tracking branch
USER 		if [ "$allowdeletebranch" != "true" ]; then
USER 			echo "*** Deleting a tracking branch is not allowed in this repository" >&2
USER 			exit 1
USER 		fi
USER 		;;
USER 	*)
USER 		# Anything else (is there anything else?)
USER 		echo "*** Update hook: unknown type of update to ref $refname of type $newrev_type" >&2
USER 		exit 1
USER 		;;
USER esac
USER 
USER # --- Finished
USER exit 0
USER ````
