USER #!/bin/sh
USER #
USER # An example hook script to make use of push options.
USER # The example simply echoes all push options that start with 'echoback='
USER # and rejects all pushes when the "reject" push option is used.
USER #
USER # To enable this hook, rename this file to "pre-receive".
USER 
USER if test -n "$GIT_PUSH_OPTION_COUNT"
USER then
USER 	i=0
USER 	while test "$i" -lt "$GIT_PUSH_OPTION_COUNT"
USER 	do
USER 		eval "value=\$GIT_PUSH_OPTION_$i"
USER 		case "$value" in
USER 		echoback=*)
USER 			echo "echo from the pre-receive-hook: ${value#*=}" >&2
USER 			;;
USER 		reject)
USER 			exit 1
USER 		esac
USER 		i=$((i + 1))
USER 	done
USER fi
