USER 	# changed since $last_update_token but not from the .git folder.
USER 	#
USER 	# To accomplish this, we're using the "since" generator to use the
USER 	# recency index to select candidate nodes and "fields" to limit the
USER 	# output to file names only. Then we're using the "expression" term to
USER 	# further constrain the results.
USER 	if (substr($last_update_token, 0, 1) eq "c") {
USER 		$last_update_token = "\"$last_update_token\"";
USER 	}
USER 	my $query = <<"	END";
USER 		["query", "$git_work_tree", {
USER 			"since": $last_update_token,
USER 			"fields": ["name"],
USER 			"expression": ["not", ["dirname", ".git"]]
