USER #!/bin/sh
USER 
USER # An example hook script to verify what is about to be pushed.  Called by "git
USER # push" after it has checked the remote status, but before anything has been
USER # pushed.  If this script exits with a non-zero status nothing will be pushed.
USER #
USER # This hook is called with the following parameters:
USER #
USER # $1 -- Name of the remote to which the push is being done
USER # $2 -- URL to which the push is being done
USER #
USER # If pushing without using a named remote those arguments will be equal.
USER #
USER # Information about the commits which are being pushed is supplied as lines to
USER # the standard input in the form:
USER #
USER #   <local ref> <local oid> <remote ref> <remote oid>
USER #
USER # This sample shows how to prevent push of commits where the log message starts
USER # with "WIP" (work in progress).
USER 
USER remote="$1"
USER url="$2"
USER 
USER zero=$(git hash-object --stdin </dev/null | tr '[0-9a-f]' '0')
USER 
USER while read local_ref local_oid remote_ref remote_oid
USER do
USER 	if test "$local_oid" = "$zero"
USER 	then
USER 		# Handle delete
USER 		:
USER 	else
USER 		if test "$remote_oid" = "$zero"
USER 		then
USER 			# New branch, examine all commits
USER 			range="$local_oid"
USER 		else
USER 			# Update to existing branch, examine new commits
USER 			range="$remote_oid..$local_oid"
USER 		fi
USER 
USER 		# Check for WIP commit
USER 		commit=$(git rev-list -n 1 --grep '^WIP' "$range")
USER 		if test -n "$commit"
USER 		then
USER 			echo >&2 "Found WIP commit in $local_ref, not pushing"
USER 			exit 1
USER 		fi
USER 	fi
USER done
USER 
USER exit 0
USER ```
